{
  "GroupName": "karen-ai-security-group",
  "Description": "Security group for Karen AI browser automation",
  "InboundRules": [
    {
      "IpProtocol": "tcp",
      "FromPort": 22,
      "ToPort": 22,
      "IpRanges": [{"CidrIp": "0.0.0.0/0"}],
      "Description": "SSH access"
    },
    {
      "IpProtocol": "tcp",
      "FromPort": 80,
      "ToPort": 80,
      "IpRanges": [{"CidrIp": "0.0.0.0/0"}],
      "Description": "HTTP access"
    },
    {
      "IpProtocol": "tcp",
      "FromPort": 443,
      "ToPort": 443,
      "IpRanges": [{"CidrIp": "0.0.0.0/0"}],
      "Description": "HTTPS access"
    },
    {
      "IpProtocol": "tcp",
      "FromPort": 3001,
      "ToPort": 3001,
      "IpRanges": [{"CidrIp": "0.0.0.0/0"}],
      "Description": "Playwright service"
    }
  ],
  "OutboundRules": [
    {
      "IpProtocol": "-1",
      "FromPort": -1,
      "ToPort": -1,
      "IpRanges": [{"CidrIp": "0.0.0.0/0"}],
      "Description": "All outbound traffic"
    }
  ]
} 